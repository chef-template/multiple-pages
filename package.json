{
  "name": "multiple-pages",
  "version": "1.0.1",
  "description": "webpack multiple pages",
  "author": "321jiangtao@gmail.com",
  "license": "MIT",
  "scripts": {
    "start": "node server",
    "build": "webpack --config build/webpack.prod.conf.js --progress --hide-modules && npm run minify",
    "test": "webpack --config build/webpack.test.conf.js --progress --hide-modules && npm run minify",
    "clean": "rm -fr dist",
    "minify": "node build/minify dist",
    "tag": "node .tag",
    "tag:delete": "git tag -d $1 && git push origin --delete tag $1"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "safari >= 7"
            ]
          },
          "loose": true
        }
      ]
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-loose": "^8.0.0",
    "css-loader": "^0.26.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "glob": "^7.1.1",
    "html-loader": "^0.4.5",
    "html-webpack-include-assets-plugin": "0.0.5",
    "html-webpack-plugin": "^2.28.0",
    "koa": "^1.4.0",
    "koa-proxy": "^0.8.0",
    "koa-router": "^5.4.1",
    "koa-webpack-dev-middleware": "^1.4.5",
    "koa-webpack-hot-middleware": "^1.0.3",
    "postcss-loader": "^1.3.3",
    "query-string": "^4.3.2",
    "style-loader": "^0.13.2",
    "uglifyjs": "2.4.10",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1"
  }
}
